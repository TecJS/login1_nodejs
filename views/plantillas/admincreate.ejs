<div class="container">
    <h2 class="mt-5">Formulario de Crear admin</h2>
    <form action="/UsuarioCreateAdmin" method="post" class="mt-4">
        <div class="form-group">
            <label for="prom_general">Nombre</label>
            <input class="form-control" id="Nombre" name="Nombre" maxlength="90" required>
        </div>
        <div class="form-group">
            <label for="prom_general">Apellidos</label>
            <input class="form-control" id="Apellidos" name="Apellidos" maxlength="90" required>
        </div>
        <div class="form-group">
            <label for="prom_general">Telefono</label>
            <input class="form-control" id="Telefono" name="Telefono" maxlength="10" required>
        </div>
        <div class="form-group">
            <label for="prom_general">Usuario</label>
            <input class="form-control" id="Usuario" name="Usuario" maxlength="40" required>
        </div>
        <div class="form-group">
            <label for="fecha">Contrasena</label>
            <input  class="form-control" id="Contrasena" name="Contrasena" maxlength="90" required>
        </div>
        <button type="submit" class="btn btn-primary">Enviar</button>
    </form>
  </div>
